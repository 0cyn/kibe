{
  "schemaVersion": 1,
  "id": "kibe",
  "version": "${version}",

  "name": "Kibe",
  "description": "A miscellaneous mod that adds a bunch of random, and mostly unoriginal things.",
  "authors": [
    "D4rkness_King"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/kibe",
    "sources": "https://github.com/lucaargolo/kibe",
    "issues": "https://github.com/lucaargolo/kibe/issues"
  },

  "license": "Mozilla Public License 2.0",
  "icon": "assets/kibe/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.lucaargolo.kibe.KibeModKt::init"
    ],
    "client": [
      "io.github.lucaargolo.kibe.KibeModKt::initClient"
    ],
    "modmenu": [
      "io.github.lucaargolo.kibe.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "kibe.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.9.2",
    "fabric": ">=0.20.1",
    "fabric-language-kotlin": ">=1.4.0",
    "minecraft": "1.16.x"
  }
}
